{
  "name": "notes-static",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.6.0"
  },
  "scripts": {
    "init-content": "node scripts/init-content.mjs",
    "predev": "node scripts/init-content.mjs",
    "prebuild": "node scripts/init-content.mjs",
    "dev": "next dev",
    "build": "NODE_ENV=production next build",
    "build:static": "NODE_ENV=production NEXT_EXPORT=true next build",
    "start": "next start",
    "preview": "npm run build:static && npx serve out",
    "lint": "eslint",
    "audit:prod": "npm audit --omit=dev",
    "test": "vitest run",
    "test:watch": "vitest",
    "watch": "node --env-file=.env scripts/watch-inbox.mjs",
    "ingest": "node --env-file=.env scripts/watch-inbox.mjs --once"
  },
  "dependencies": {
    "@tailwindcss/typography": "^0.5.19",
    "fuse.js": "^7.1.0",
    "gray-matter": "^4.0.3",
    "next": "16.1.6",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "remark": "^15.0.1",
    "remark-html": "^16.0.1"
  },
  "devDependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "@google/genai": "^0.7.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.1.4",
    "chokidar": "^4.0.3",
    "eslint": "^9.39.2",
    "eslint-config-next": "16.1.6",
    "jsdom": "^28.1.0",
    "openai": "^4.104.0",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^4.0.18"
  }
}
